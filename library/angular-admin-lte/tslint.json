{
    "extends": "../../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "mk",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "mk",
            "kebab-case"
        ],
      "no-input-rename": false,
      "no-output-rename": false,
      "no-output-on-prefix": false
    }
}
